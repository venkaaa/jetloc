/* ========================================================================
                                START CONTACT-BOX
========================================================================= */
.contact-box {
    background: url('../images/elements/bg-5.jpg') no-repeat top center fixed;
    margin-bottom: -60px;
}
.contact-box .contact-box-bg {
    padding: 60px 0px;
